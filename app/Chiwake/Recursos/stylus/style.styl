/* 22.   Preloader
--------------------------------------------------------------------------------*/
.preloader
	position fixed
	width 100%
	height 100%
	top 0
	left 0
	text-align center
	z-index 9999999

.preloader:before, .preloader:after
	content ''
	display block
	position absolute
	width 130%
	height 50%
	background-color #111
	left -15%
	transition all 2s ease 0.5s

.preloader:before
	top 0
	transform-origin 0 100%

.preloader:after
	bottom 0
	transform-origin 0 0

.preloader .inner
	position absolute
	display inline-block
	width 300px
	padding 50px 0
	top 50%
	left 50%
	transform translate(-50%, -50%)
	z-index 999
	opacity 1
	visibility visible
	transition all 1s ease

.preloader .inner .item
	width 10px
	height 10px
	border-radius 50%
	background-color $colorPr
	display inline-block
	margin 10px
	animation preanim 1.5s linear infinite

.preloader .inner .item2
	animation-delay 0.3s

.preloader .inner .item3
	animation-delay 0.6s

.preloader.load-anim
	visibility hidden

.preloader.load-anim:before
	transform translateX(-100%)

.preloader.load-anim:after
	transform translateX(100%)

.preloader.load-anim .inner
	opacity 0
	visibility hidden


@keyframes preanim
	40%
		transform scale(3)
  



