@font-face {
    font-family: 'signpaintermedium';
    src: url('/font/signpainter/signpainter-housescript-webfont.eot');
    src: url('/font/signpainter/signpainter-housescript-webfont.eot?#iefix') format('embedded-opentype'),
         url('/font/signpainter/signpainter-housescript-webfont.woff2') format('woff2'),
         url('/font/signpainter/signpainter-housescript-webfont.woff') format('woff'),
         url('/font/signpainter/signpainter-housescript-webfont.ttf') format('truetype'),
         url('/font/signpainter/signpainter-housescript-webfont.svg#signpaintermedium') format('svg');
    font-weight: normal;
    font-style: normal;
}

